GLUETUN_CONTAINER=        # Container name for the Gluetun VPN (e.g. "gluetun")
VPN_PROVIDER=             # VPN service (e.g. "windscribe")
WIND_USERNAME=            # Windscribe VPN username
WIND_PASSWORD=            # Windscribe VPN password
WIND_COUNTRY=             # Preferred country/server (e.g. "Australia")
TZ=                       # Timezone (e.g. "Australia/Sydney")
GLUETUN_VOLUME=           # Host path to store Gluetun config (e.g. "./gluetun")
GLUETUN_MOUNT=            # Mount path inside the container (e.g. "/gluetun")

HEALTHCHECK_HOST=         # Host to ping for healthcheck (e.g. "github.com")
HEALTHCHECK_INTERVAL=     # Interval for healthcheck (e.g. "30s")
HEALTHCHECK_TIMEOUT=      # Timeout for healthcheck (e.g. "10s")
HEALTHCHECK_RETRIES=      # Retry count before marking as unhealthy (e.g. "5")

WINDOWS_CONTAINER=        # Container name for the Windows VM (e.g. "windows")
WINDOWS_IMAGE=            # Image name (e.g. "dockurr/windows")
WINDOWS_HOSTNAME=         # Internal hostname (e.g. "illusion")
WINDOWS_VERSION=          # Windows version (e.g. "11")
WINDOWS_VOLUME=           # Host path for persistent storage (e.g. "./windows")
WINDOWS_MOUNT=            # Mount path inside container (e.g. "/storage")

KVM_DEVICE=               # Path to KVM device (e.g. "/dev/kvm")
TUN_DEVICE=               # Path to tun device (e.g. "/dev/net/tun")

WINDOWS_WEB_PORT=         # Web interface port (e.g. "8006")
RDP_PORT=                 # RDP port (e.g. "3389")

STOP_GRACE_PERIOD=        # Graceful stop timeout (e.g. "2m")